{
  "name": "rabota-md-parser",
  "version": "0.1.0",
  "description": "Job vacancy parser for rabota.md and other sites",
  "type": "module",
  "engines": {
    "node": ">= 22.11 < 23"
  },
  "devDependencies": {
    "@eslint/js": "~9.17",
    "@types/eslint__js": "~8.42",
    "@types/jsdom": "~21.1",
    "@types/node": "~20",
    "@typescript-eslint/parser": "~8.19",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "~1.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "~9.1",
    "globals": "~15.14",
    "prettier": "~3.4",
    "rimraf": "~6.0",
    "ts-api-utils": "~2.0",
    "typescript": "~5.7",
    "typescript-eslint": "~8.19",
    "vitest": "^4.0.18"
  },
  "scripts": {
    "start": "docker-compose up -d && concurrently \"npm run start:api\" \"npm run start:worker\"",
    "dev": "docker-compose up -d && concurrently \"npm run dev:api\" \"npm run dev:worker\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "start:999": "node build/src/test999.js",
    "parse": "node build/src/parse.js",
    "manage": "node build/src/manageVacancies.js",
    "test:puppeteer": "node build/src/testPuppeteer.js",
    "dev:api": "tsx src/api/server.ts",
    "dev:worker": "tsx src/worker/worker.ts",
    "dict:update": "tsx src/utils/dictionaries/index.ts",
    "test:system": "tsx test-system.ts",
    "test:managers": "tsx test-managers.ts",
    "start:api": "node build/src/api/server.js",
    "start:worker": "node build/src/worker/worker.js",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint .",
    "test": "vitest run unit --config tests/vitest.config.ts",
    "test:coverage": "vitest run unit --config tests/vitest.config.ts --coverage.enabled --coverage.all",
    "prettier": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --write",
    "prettier:check": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --check",
    "test:watch": "vitest unit",
    "mode": "tsx scripts/switch-mode.ts",
    "mode:dev": "tsx scripts/switch-mode.ts dev",
    "mode:prod": "tsx scripts/switch-mode.ts prod",
    "init": "tsx scripts/init.ts"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@prisma/client": "^6.19.1",
    "@types/puppeteer": "^5.4.7",
    "axios": "^1.6.7",
    "bullmq": "^5.66.4",
    "cheerio": "^1.0.0-rc.12",
    "cloc": "^2.6.0-cloc",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "fuse.js": "^7.1.0",
    "ioredis": "^5.8.2",
    "jsdom": "^23.0.0",
    "p-limit": "^7.2.0",
    "prisma": "^6.19.1",
    "puppeteer": "^24.34.0",
    "redis": "^5.10.0",
    "tslib": "~2.8",
    "tsx": "^4.21.0"
  },
  "volta": {
    "node": "22.12.0"
  }
}
